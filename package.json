{
  "name": "react-query-infinite-scroll",
  "version": "1.0.4",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "import": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "module": "./dist/index.esm.js"
  },
  "files": [
    "./dist/**/*.{js,d.ts,mjs}"
  ],
  "scripts": {
    "start": "parcel ./example/index.html",
    "build": "bunchee ./src/index.ts",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "repository": "github:qqww08/react-query-infinite-scroll",
  "keywords": [
    "react",
    "react-query",
    "infinite scroll"
  ],
  "author": "qqww08",
  "license": "MIT",
  "homepage": "https://github.com/qqww08/react-query-infinite-scroll",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "bunchee": "^2.2.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^21.0.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --cache"
    ]
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.0.0",
    "react": "^16.11.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.11.0 || ^17.0.0 || ^18.0.0",
    "react-query": "^3.0.0"
  },
  "dependencies": {
    "react-intersection-observer": "^9.4.3"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}
